{
  "name": "line-chart-visualizer",
  "version": "1.1.0",
  "description": "line-chart-visualizer is a flexible and easy-to-use React chart library for rendering line charts, developed by Varol Cagdas Tok. It allows for extensive customization, including grid display, axis labels, annotations, and filling areas between lines with color.",
  "main": "dist/LineChart.js",
  "exports": {
    ".": "./dist/LineChart.js",
    "./react": "./dist/ReactLineChart.js"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.js'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctokx/line-chart-visualizer"
  },
  "keywords": [
    "javascript",
    "react",
    "chart",
    "linechart",
    "data-visualization",
    "canvas",
    "plot",
    "graph"
  ],
  "author": "Varol Cagdas Tok",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  }
}
